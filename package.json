{
  "name": "@sergkhl/next-fusion",
  "description": "Personal Website",
  "version": "1.0.0",
  "repository": "https://github.com/sergkhl/next-fusion",
  "scripts": {
    "dev:server": "nodemon",
    "serve": "ts-node --project tsconfig.server.json server/serve.ts",
    "dev": "next",
    "build:ssr": "next build && tsc --project tsconfig.server.json",
    "build": "next build && next export",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
    "pretty": "prettier --write --config .prettierrc.js {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "export": "cross-env NODE_ENV=production next export",
    "storybook": "start-storybook -s ./static/storybook -p 6006",
    "build-storybook": "build-storybook -s ./static/storybook  -c .storybook -o .storybookout",
    "graphql:codegen": "cross-env NODE_ENV=staging graphql-codegen --config scripts/codegen.js",
    "update-packages": "npx npm-check-updates -u >> package-update.log"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^3.1.5",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "acorn": "^7.2.0",
    "apollo-boost": "^0.4.8",
    "autoprefixer": "^9.7.6",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "drei": "0.0.32",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "http-proxy-middleware": "^1.0.3",
    "isomorphic-unfetch": "^3.0.0",
    "lerp": "^1.0.3",
    "next": "^9.4.0",
    "next-compose-plugins": "^2.2.0",
    "next-redux-wrapper": "^6.0.0",
    "next-runtime-dotenv": "^1.2.0",
    "node-sass": "^4.14.1",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-three-fiber": "^4.2.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.0",
    "three": "^0.116.1",
    "threejs-meshline": "^2.0.11"
  },
