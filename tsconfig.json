{
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "declaration": false,
    "sourceMap": true,
    "target": "esnext",
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@Actions": ["src/Actions"],
      "@Components/*": ["src/Components/*"],
      "@Components": ["src/Components"],
      "@Definitions/*": ["src/Definitions/*"],
      "@Definitions": ["src/Definitions"],
      "@Interfaces": ["src/Interfaces"],
      "@Interfaces/*": ["src/Interfaces/*"],
      "@Pages/*": ["pages/*"],
      "@Reducers": ["src/Redux/Reducers"],
      "@Reducers/*": ["src/Redux/Reducers/*"],
      "@Redux": ["src/Redux"],
      "@Redux/*": ["src/Redux/*"],
      "@Store": ["src/Store"],
      "@Store/*": ["src/Store/*"],
      "@Server/*": ["server/*"],
      "@Services": ["src/Services"],
      "@Services/*": ["src/Services/*"],
      "@Public/*": ["public/*"],
      "@Styled/*": ["src/Styled/*"],
      "@Test/*": ["test/*"]
    }
  },
  "exclude": [
    // "node_modules/**/*.js",
    "dist",
    ".next",
    "out",
    ".babelrc",
    "bundles",
    "coverage",
    "test/*"
  ],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.js"]
}
